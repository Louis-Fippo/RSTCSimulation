drawSimNew <-function(JeuxDonneASB,nomf)
{
  #inclusion des fichiers sources
 
  source("drawAllNew.R");
  
  borne=c(1,5,9,13,17,21,25);
  print("ici")
  for(i in borne)
  {
    
    drawAllNew(JeuxDonneASB,i,nomf);
    
  }
  
}
